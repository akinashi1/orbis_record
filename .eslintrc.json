{
    "extends": ["next/core-web-vitals", "next/typescript"],

	"rules": {
		"no-unused-expressions": "off"
	},

	"rules": {
        "no-unused-vars": ["error", {
            "vars": false,
            "args": false,
            "caughtErrors": false,
            "ignoreRestSiblings": false,
            "reportUsedIgnorePattern": false
        }]
    }
}
